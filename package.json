{
  "name": "gtai",
  "description": "CLI tool for Google Translate Advanced LLM",
  "version": "1.2.3",
  "main": "dist/index.js",
  "bin": {
    "gtai": "./dist/index.js"
  },
  "type": "module",
  "engines": {
    "bun": ">=1.2.17"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "translate",
    "google-translate",
    "cli",
    "bun",
    "translation",
    "glossary",
    "i18n"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outfile ./dist/index.js --target bun",
    "dev": "bun run ./src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint ./src"
  },
  "devDependencies": {
    "@clack/prompts": "1.0.0-alpha.1",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/translate": "^9.1.0",
    "@types/bun": "^1.2.18",
    "clipboardy": "^4.0.0",
    "oxlint": "^1.5.0",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": [
    "protobufjs"
  ],
  "author": "phenomen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomen/gtai.git"
  },
  "bugs": {
    "url": "https://github.com/phenomen/gtai/issues"
  },
  "homepage": "https://github.com/phenomen/gtai#readme"
}
